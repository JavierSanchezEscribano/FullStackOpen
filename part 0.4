    activate server
    server-->>browser: CSS file
    deactivate server

    browser->>server: GET https://studies.cs.helsinki.fi/exampleapp/main.js
    note right of browser: Get the new JavaScript file
    activate server
    server-->>browser: JavaScript file
    deactivate server

    browser->>server: GET https://studies.cs.helsinki.fi/exampleapp/data.json
    note right of browser: Get the new JSON file
    activate server
    server-->>browser: [{ "content": "new note", "date": "XXXX-XX-XX" }, ... ]
    deactivate server
